{
  "name": "openapi-viewer",
  "displayName": "Openapi viewer",
  "version": "0.0.1",
  "description": "A OpenAPI viewer on GitHub",
  "author": "kbwo",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "core-js": "^3.28.0",
    "js-yaml": "^4.1.0",
    "mobx": "^6.8.0",
    "plasmo": "0.65.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "^16.8",
    "redoc": "^2.0.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.1",
    "@types/chrome": "0.0.210",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "prettier": "2.8.3",
    "typescript": "4.9.4"
  },
  "manifest": {
    "host_permissions": [
      "https://github.com/*"
    ],
    "permissions": [
      "tabs",
      "storage",
      "scripting",
      "webNavigation"
    ]
  }
}
